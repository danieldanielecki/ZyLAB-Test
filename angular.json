        }
      }
    },
    "authors": {
      "root": "libs/authors",
      "sourceRoot": "libs/authors/src",
      "projectType": "library",
      "prefix": "zylab-workspace",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
